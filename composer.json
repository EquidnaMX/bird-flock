{
  "name": "equidna/bird-flock",
  "version": "1.0.0",
  "description": "Bird Flock - messaging bus for multi-channel outbound messaging (SMS, WhatsApp, Email) with DLQ and idempotency",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Ruelas",
      "email": "gruelas@gruelas.com"
    }
  ],
  "require": {
    "php": ">=8.3",
    "laravel/framework": "^11.0",
    "illuminate/support": "^10 || ^11 || ^12",
    "illuminate/queue": "^10 || ^11 || ^12",
    "illuminate/events": "^10 || ^11 || ^12",
    "illuminate/http": "^10 || ^11 || ^12",
    "illuminate/config": "^10 || ^11 || ^12",
    "illuminate/routing": "^10 || ^11 || ^12",
    "symfony/uid": "^7.0",
    "psr/log": "3.0",
    "guzzlehttp/guzzle": "^7.0",
    "twilio/sdk": "^6.0",
    "sendgrid/sendgrid": "^7.0",
    "vonage/client": "^4.2",
    "mailgun/mailgun-php": "^4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "squizlabs/php_codesniffer": "^4.0",
    "illuminate/database": "^11.0",
    "phpstan/phpstan": "^1.10",
    "phpstan/extension-installer": "^1.3",
    "phpstan/phpstan-phpunit": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Equidna\\BirdFlock\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Equidna\\BirdFlock\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Equidna\\BirdFlock\\BirdFlockServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "phpstan/extension-installer": true
    }
  }
}
